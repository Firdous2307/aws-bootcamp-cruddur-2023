#! /usr/bin/bash
set -e # stop if it fails at any point

CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL="db-setup"
printf "${CYAN}==== ${LABEL}${NO_COLOR}\n"


ABS_PATH=$(readlink -f "$0")
bin_path=$(dirname $ABS_PATH)

source "$DB_path/drop"
source "$DB_path/create"
source "$DB_path/schema-load"
source "$DB_path/seed"

python "$DB_path/update_cognito_user_ids"